DROP TABLE admin_klinike;
CREATE TABLE admin_klinike (
   id_admin INT NOT NULL AUTO_INCREMENT,
   email VARCHAR(50),
   lozinka VARCHAR(50),
   ime VARCHAR(50),
   prezime VARCHAR(50),
   aktivan BOOLEAN,
   prvo_logovanje BOOLEAN,
   klinika INT
);

